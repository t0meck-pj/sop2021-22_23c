#include<stdio.h>
#include <string.h>
#include <stdlib.h>

int main (int argc, char *argv[] ){

    int i, j,w;

	w = atoi(argv[1]);

    for (i = w ; i > 0; i--) {
        for (j = 0; j < i; j++)
            printf (" ");
        for (j = i; j < w+1; j++)
            printf ("**");
        printf ("\n");

    }

    return 0;

}
